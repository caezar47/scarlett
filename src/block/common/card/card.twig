{% if view == "faq" %}
	<div class="{{prefix}}group  panel-group  {{mod}}" id="{{card_id}}">
		{% for v in list %}
			{% for value in v.list %}
				<div class="{{prefix}}item  panel  {{mod}}">
					<a class="{{prefix}}heading  {{mod}}" data-toggle="collapse" data-parent="#{{card_id}}" href="#{{card_id}}{{value.id}}" aria-expanded="false">{{value.name}}</a>
					<div id="{{card_id}}{{value.id}}" class="collapse">
						<div class="{{prefix}}note  text__block  {{mod}}">{{value.note}}</div>
					</div>
				</div>
			{% endfor %}
		{% endfor %}
	</div>		
{% endif %}
{% if view == "card" %}
	{% for value in list %}		
	<div class="{{prefix}}item  {{mod}}">
		<div class="{{prefix}}preview  {{mod}}">
			<img src="{{value.img}}" alt="{{value.heading}}" class="img-responsive">
		</div>
		<p class="{{prefix}}heading  {{mod}}">{{value.heading}}</p>
		<p class="{{prefix}}label  {{mod}}">{{value.label}}</p>
		{% if value.note %}<p class="{{prefix}}note  {{mod}}">{{value.note}}</p>{% endif %}
	</div> 
	{% endfor %}
{% endif %}
{# old #}
{% if view == "works_card" %}
	{% for value in list %}		
	<a href="{{value.link}}" class="{{prefix}}item  {{mod}}" {% if aos %}data-aos="{{aos}}"{% endif %}>
		<div class="{{prefix}}preview  {{mod}}">
			<img data-src="{{value.img}}" alt="{{value.heading}}" class="img-responsive  lazy">
		</div>
		<p class="{{prefix}}heading  {{mod}}">
			<span>{{value.name}}</span>
			<span class="{{prefix}}icon  {{mod}}">
				{% include "../svg-sprite/svg.twig" with {icon:"arrow-next"} %}
			</span>
		</p>
		{% if value.note %}<p class="{{prefix}}note  {{mod}}">{{value.note}}</p>{% endif %}		
		{% if value.label %}<p class="{{prefix}}label  {{mod}}">{{value.label}}</p>{% endif %}
	</a> 
	{% endfor %}
{% endif %}

{% if view == "works_gallery" %}
	<div class="{{prefix}}group  {{mod}}">
		{% for value in list %}		
		<div class="{{prefix}}item  {{mod}}">
			<div class="{{prefix}}row  {{mod}}  {{value.type}}">
				{% for v in value.sublist %}
				<div class="{{prefix}}preview  {{mod}}">
					{% if v.video %}
					<video autoplay loop playsinline muted>
						<source src="{{v.video}}" type="video/mp4">	
					</video>
					{% else %}
					<img data-src="{{v.img}}" alt="" class="img-responsive  lazy">					
					{% endif %}
				</div>
			{% endfor %}
			</div>
			{% if value.note %}<div class="{{prefix}}note  {{mod}}">{{value.note}}</div>{% endif %}		
		</div> 
		{% endfor %}
	</div> 
{% endif %}

{% if view == "works_card_xs" %}
	<div class="{{prefix}}row  {{mod}}">
		{% for value in list %}		
		<a href="{{value.link}}" class="{{prefix}}item  {{mod}}">
			<div class="{{prefix}}preview  {{mod}}">
				<img src="{{value.img}}" alt="{{value.heading}}" class="img-responsive">
			</div>
		</a> 
		{% endfor %}
	</div>
{% endif %}

